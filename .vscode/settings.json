// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/app/**/*.js": {
        "when": "$(basename).ts"
    },
    "**/app/**/*.css": {
        "when": "$(basename).scss"
    }
  }
}